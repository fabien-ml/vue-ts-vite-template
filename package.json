{
  "name": "vue-ts-vite-template",
  "version": "0.1.0",
  "author": {
    "name": "Fabien MARIE-LOUISE",
    "url": "https://github.com/fabien-ml"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview --port 3000",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "test:e2e": "start-server-and-test serve 3000 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve 3000 'cypress run'",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,vue}\" && git update-index --again",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.1.1",
    "@cypress/vue": "^3.0.3",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.0",
    "cypress": "^8.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.0.2",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
